{
	"name": "sdv-downgrader",
	"productName": "SDV Downgrader",
	"version": "v0.0.1",
	"description": "Automate downloading older versions of Stardew Valley for speedrunning.",
	"license": "MIT",
	"repository": "Ryah/SDV-Downgrader",
	"author": {
		"name": "Ryah",
		"email": "14987609+Ryah@users.noreply.github.com",
		"url": "https://github.com/Ryah/"
	},
	"scripts": {
		"postinstall": "electron-builder install-app-deps",
		"lint": "xo",
		"test": "npm run lint",
		"start": "electron .",
		"pack": "electron-builder --dir",
		"dist": "electron-builder --macos --linux --windows",
		"release": "np"
	},
	"dependencies": {
		"adm-zip": "^0.5.9",
		"bootstrap": "^5.1.3",
		"electron-context-menu": "^3.0.0",
		"electron-debug": "^3.2.0",
		"electron-store": "^8.0.0",
		"electron-unhandled": "^3.0.2",
		"electron-updater": "^4.3.8",
		"electron-util": "^0.15.1",
		"events": "^3.3.0",
		"jquery": "^3.6.0",
		"popper.js": "^1.16.1",
		"request": "^2.88.2",
		"xterm": "^4.15.0"
	},
	"devDependencies": {
		"electron": "^12.0.6",
		"electron-builder": "^22.10.5",
		"np": "^7.5.0",
		"xo": "^0.39.1"
	},
	"xo": {
		"envs": [
			"node",
			"browser"
		]
	},
	"np": {
		"publish": false,
		"releaseDraft": false
	},
	"build": {
		"appId": "com.Ryah.SDVDowngrader",
		"mac": {
			"category": "public.app-category.social-networking",
			"darkModeSupport": true
		},
		"dmg": {
			"iconSize": 160,
			"contents": [
				{
					"x": 180,
					"y": 170
				},
				{
					"x": 480,
					"y": 170,
					"type": "link",
					"path": "/Applications"
				}
			]
		},
		"linux": {
			"target": [
				"AppImage",
				"deb"
			],
			"category": "Network;Chat"
		}
	}
}
